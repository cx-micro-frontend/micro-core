{
  "name": "neap-core",
  "neap-tag": "20201201-g01",
  "version": "0.0.1-14",
  "description": "neap-core",
  "author": "Broccoli spring( gcx )",
  "private": false,
  "scripts": {
    "command": "node_modules\\cx-builder\\lib\\entry.sh root",
    "command:mac": "node_modules/cx-builder/lib/entry.sh root",
    "inject": "node ./lib/injecter/index.js",
    "build:dist": "cross-env NODE_ENV=production env_config=prod node node_modules/cx-builder/lib/run/prod/run.prod.js",
    "clean:dist": "rimraf ./dist/",
    "build:injecter": "node build/create-injecter.js --color",
    "build:lib": "node build/create-lib.js --color",
    "clean:lib": "rimraf ./lib/",
    "upgrade": "node_modules/cx-builder/lib/entry.sh root 6 3",
    "reinstall": "node_modules/cx-builder/lib/entry.sh root 6 4",
    "fix-memory-limit": "cross-env LIMIT=8096 increase-memory-limit",
    "svgo": "svgo -f config/expand/iconfont/module-menu/ --config=config/expand/iconfont/svgo.yml",
    "nvm": "nvm"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "clipboard": "^2.0.6",
    "core-js": "^3.1.4",
    "crypto-js": "^3.1.9-1",
    "cx-theme": "^1.0.5",
    "davidshimjs-qrcodejs": "^0.0.2",
    "echarts": "^4.2.1",
    "element-resize-detector": "^1.2.1",
    "element-ui": "2.11.1",
    "increase-memory-limit": "^1.0.7",
    "jquery": "3.4.1",
    "js-cookie": "^2.2.0",
    "neap-ui": "0.0.2-9",
    "nprogress": "^0.2.0",
    "vue-i18n": "^5.0.3",
    "vue-material-year-calendar": "^1.2.4",
    "vue-native-websocket": "^2.0.14",
    "vue-router": "3.1.6",
    "vue2-editor": "^2.10.2",
    "vuex": "^2.3.1",
    "xlsx": "^0.11.0"
  },
  "devDependencies": {
    "@vant/compiler": "^1.0.0",
    "babel-preset-es2015": "^6.24.1",
    "cx-builder": "0.0.53-46",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.3",
    "gulp-notify": "^3.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.1",
    "mockjs": "^1.0.1-beta3",
    "vue": "2.6.10",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "main/src/**/*.{js,json,jsx,less}": [
      "node node_modules/cx-builder/lib/prettier/lint-prettier.js",
      "git add"
    ],
    "main/*.{js,jsx}": "eslint --ext .js",
    "main/*.{less,scss,sass}": "stylelint --syntax less",
    "config/**/*.{js,json}": [
      "node node_modules/cx-builder/lib/prettier/lint-prettier.js",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
