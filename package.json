{
  "name": "neap-core",
  "neap-tag": "20210611-g01",
  "version": "0.0.1-89",
  "description": "neap-core",
  "author": "Broccoli spring( gcx )",
  "private": false,
  "scripts": {
    "command": "node_modules\\cx-builder\\lib\\entry.sh root",
    "command:mac": "node_modules/cx-builder/lib/entry.sh root",
    "inject": "cross-env node ./lib/injecter/index.js",
    "build:dist": "cross-env NODE_ENV=production env_config=prod node node_modules/cx-builder/lib/run/prod/run.prod.js",
    "build:jenkins": "cross-env NODE_ENV=production env_config=prod PROD_ENV=jenkins node node_modules/cx-builder/lib/run/prod/run.prod.js",
    "clean:dist": "rimraf ./dist/",
    "build:injecter": "node build/src/create-injecter.js --color",
    "build:lib": "node build/src/create-lib.js --color",
    "upgrade": "node_modules/cx-builder/lib/entry.sh root 6 3",
    "reinstall": "node_modules/cx-builder/lib/entry.sh root 6 4",
    "fix-memory-limit": "cross-env LIMIT=16384 increase-memory-limit",
    "svgo": "svgo -f config/expand/iconfont/module-menu/ --config=config/expand/iconfont/svgo.yml",
    "nvm": "nvm",
    "fix": "./fix.sh",
    "set-permission-sh": "chmod +x ./fix.sh; chmod +x node_modules/cx-builder/lib/entry.sh",
    "jekines": "./jekinesShell.sh 'all,portal,design_center,o2o_admin,work_order' 'bill,refund,Meter,report,pay,job' 'a,b,c,d,e,f,g'",
    "compile": "node ./build/src/compile.js  --color"
  },
  "license": "ISC",
  "dependencies": {
    "@antv/f2": "^3.8.1",
    "@antv/g2": "^4.1.5",
    "animate.css": "^4.1.1",
    "axios": "^0.16.2",
    "clipboard": "^2.0.6",
    "core-js": "^3.1.4",
    "cross-env": "^7.0.3",
    "crypto-js": "^3.1.9-1",
    "cx-theme": "^1.0.5",
    "davidshimjs-qrcodejs": "^0.0.2",
    "echarts": "^4.2.1",
    "element-resize-detector": "^1.2.1",
    "element-ui": "2.11.1",
    "increase-memory-limit": "^1.0.7",
    "jquery": "3.4.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.29.1",
    "neap-ui": "0.0.2-27",
    "nprogress": "^0.2.0",
    "push.js": "^1.0.12",
    "uuid": "^8.3.2",
    "vant": "^2.12.2",
    "vue-i18n": "^5.0.3",
    "vue-material-year-calendar": "^1.2.4",
    "vue-native-websocket": "^2.0.14",
    "vue-router": "3.1.6",
    "vue2-editor": "^2.10.2",
    "vuedraggable": "^2.24.3",
    "vuex": "^2.3.1",
    "xlsx": "^0.11.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "cx-builder": "0.0.53-54",
    "mockjs": "^1.0.1-beta3",
    "vue": "2.6.10",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "main/src/**/*.{js,json,jsx,less}": [
      "node node_modules/cx-builder/lib/prettier/lint-prettier.js",
      "git add"
    ],
    "main/*.{js,jsx}": "eslint --ext .js",
    "main/*.{less,scss,sass}": "stylelint --syntax less",
    "config/**/*.{js,json}": [
      "node node_modules/cx-builder/lib/prettier/lint-prettier.js",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
